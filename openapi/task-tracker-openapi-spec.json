  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "emailAddress": {
            "type": "string"
          }
        }
      },
      "UserInfo": {
        "required": [
          "email",
          "firstName",
          "lastName",
          "nickname",
          "picture"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "nickname": {
            "type": "string"
          },
          "picture": {
            "type": "string"
          }
        }
      },
      "Link": {
        "type": "object",
        "properties": {
          "href": {
            "type": "string"
          },
          "templated": {
            "type": "boolean"
          }
        }
      }
    },
    "securitySchemes": {
      "OAuth2": {
        "type": "oauth2",
        "flows": {
          "password": {
            "authorizationUrl": "https://dajohnston.eu.auth0.com/oauth/authorize",
            "tokenUrl": "https://dajohnston.eu.auth0.com/oauth/token",
            "scopes": {
              "offline_access": ""
            }
          }
        }
      }
    }
  }
}