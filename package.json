    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unicorn": "^50.0.1",
    "jsdom": "^24.0.0",
    "prettier": "3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.13",
    "vitest": "^2.0.4"
  }
