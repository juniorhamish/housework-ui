    "eslint-plugin-unicorn": "^50.0.1",
    "jsdom": "^24.1.1",
    "prettier": "3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
